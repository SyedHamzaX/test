{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/login",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/login.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}